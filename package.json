{
  "name": "embedded-browser-react",
  "version": "0.0.1",
  "description": "To running a web preview within your browser",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --open --mode development",
    "build": "babel src/index.js -o ./index.js && cp ./src/styles.css .",
    "build:publish": "babel src/index.js -o ./index.js && cp ./src/styles.css . && npm publish --access public"
  },
  "author": "Sakthivel M <msv300@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.10.3",
    "@babel/core": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-react": "^7.10.1",
    "babel-loader": "^8.1.0",
    "babel-preset-minify": "^0.5.1",
    "css-loader": "^3.6.0",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.3.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "style-loader": "^1.2.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  }
